#include <stdio.h>
void l_search(int i,a[},int size,int ele)
{  for (i=0;i<size;i++)
{ if (a[i]==ele)
{ flag+=;}
}printf("the no. value found is = %d",flag);
}
int main(){
int i,a[100],size,ele:
printf("enter the size of array");
scanf("%d",&size);
for(i=0;i<size;i++){
scanf("%d",&a[]);}
printf("enter the value of ele");
scanf("%d",&ele);
l_search (a,size,ele);

return 0;
}
